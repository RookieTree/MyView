<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tree.myview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RulerDemo">
        <activity android:name="com.tree.myview.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tree.myview.BottomViewActivity"/>
        <activity android:name="com.tree.myview.RecordViewActivity"/>
        <activity android:name="com.tree.myview.SoundRippleActivity"/>
        <activity android:name="com.tree.myview.MotionBallActivity"/>
        <activity android:name="com.tree.myview.RefreshActivity"/>
    </application>

</manifest>